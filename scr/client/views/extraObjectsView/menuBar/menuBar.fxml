<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="50.0" prefWidth="1024.0" spacing="10.0" style="-fx-background-color: #e5e5e5#e5e5e5;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.extraObjectsView.menuBar.MenuBarController">
   <children>
      <Text fx:id="logText" fill="#0660e8" onMouseClicked="#logInAction" strokeType="OUTSIDE" strokeWidth="0.0" text="LOG">
         <font>
            <Font name="Arial Rounded MT Bold" size="21.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <HBox.margin>
            <Insets left="20.0" right="20.0" />
         </HBox.margin>
      </Text>
      <Text fx:id="text1" fill="#0660e8" onMouseClicked="#onMyAccount" strokeType="OUTSIDE" strokeWidth="0.0" text="Text1">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="text2" fill="#0660e8" onMouseClicked="#onManageAccount" strokeType="OUTSIDE" strokeWidth="0.0" text="Text2">
         <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="text3" fill="#0660e8" onMouseClicked="#onManageReservations" strokeType="OUTSIDE" strokeWidth="0.0" text="Text3">
         <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="text4" fill="#0660e8" onMouseClicked="#onManageCars" strokeType="OUTSIDE" strokeWidth="0.0" text="Text4">
         <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <HBox fx:id="welcomeHBox" alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="725.0" spacing="10.0" HBox.hgrow="ALWAYS">
         <children>
            <Text fx:id="nameText" strokeType="OUTSIDE" strokeWidth="0.0" text="[NAME]">
               <font>
                  <Font size="27.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome">
               <font>
                  <Font name="Arial Rounded MT Bold" size="26.0" />
               </font>
            </Text>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
      <ImageView fitHeight="32.0" fitWidth="28.0" onMouseClicked="#goHome" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="30.0" top="10.0" />
         </HBox.margin>
         <image>
            <Image url="@../../utils/images/home.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
</HBox>
